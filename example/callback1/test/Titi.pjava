package test;

import popjava.annotation.POPClass;
import popjava.annotation.POPObjectDescription;
import popjava.annotation.POPSyncSeq;
import popjava.baseobject.ConnectionType;
import popjava.util.LogWriter;

@POPClass
public class Titi {
private int identity;
   private Toto toto;
   
   @POPObjectDescription(url="192.168.1.105", connection=ConnectionType.DEAMON)
	public Titi(){
		identity = -1;
	}
	
	
   @POPSyncSeq
	public void setIdent(int i){
		identity = i;
	}

   @POPSyncSeq
   public void setToto(Toto t){
		LogWriter.writeDebugInfo("*****"+t.getAccessPoint());
		toto = t;
   }
   
   @POPSyncSeq
	public void computeIdent(){
		toto.setIdent(identity);
	}
}

